{
  "name": "notion-puller",
  "version": "0.2.0",
  "description": "Pull Notion pages and databases to local markdown and CSV files",
  "type": "module",
  "bin": {
    "notion-puller": "./dist/index.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/bun": "latest",
    "@types/node": "^25.2.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@notionhq/client": "^5.9.0",
    "commander": "^14.0.3"
  }
}
